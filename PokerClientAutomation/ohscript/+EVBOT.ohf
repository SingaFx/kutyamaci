$$cr1 > f$TurnCard1 && (($$cr1 < $$cr0 && $$cr1 < $$cr2) || ($$cr1 < $$cr0 && $$cr1 < $$cr3) || ($$cr1 < $$cr2 && $$cr1 < $$cr3)) ? $$cr1 :
$$cr2 > f$TurnCard1 && (($$cr2 < $$cr0 && $$cr2 < $$cr1) || ($$cr2 < $$cr0 && $$cr2 < $$cr3) || ($$cr2 < $$cr1 && $$cr2 < $$cr3)) ? $$cr2 :
$$cr3 > f$TurnCard1 && (($$cr3 < $$cr0 && $$cr3 < $$cr1) || ($$cr3 < $$cr0 && $$cr3 < $$cr2) || ($$cr3 < $$cr1 && $$cr3 < $$cr2)) ? $$cr3 : 0

