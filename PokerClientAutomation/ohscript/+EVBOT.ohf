					f$IsTurnSet || f$IsTurnStraight || f$IsTurnFlush || f$IsTurnFull || f$IsTurnStrongPoker || isstraightflush ||
					f$IsTurnStrongTopPair || f$IsTurnOverPair
			:f$OpponentHandStrength == 2 ?
					f$StackSize == 0 ? 
						f$IsFlushDraw || (f$IsOESD && nsuitedcommon < 3) || f$IsTurnTwoPair || f$IsTurnDrill ||
						f$IsTurnSet || f$IsTurnStraight || f$IsTurnFlush || f$IsTurnFull || f$IsTurnStrongPoker || isstraightflush ||
						f$IsTurnTopPair || f$IsTurnOverPair
					:f$StackSize == 1 ? 
						f$IsMonsterDraw || f$IsTurnTwoPair || f$IsTurnDrill ||
						f$IsTurnSet || f$IsTurnStraight || f$IsTurnFlush || f$IsTurnFull || f$IsTurnStrongPoker || isstraightflush ||
						f$IsTurnStrongTopPair || f$IsTurnOverPair
					:f$StackSize == 2 ?
						(f$IsTurnStrongTwoPair && !f$IsFlopDangerousBoard) || (f$IsTurnStrongDrill && !f$IsFlopDangerousBoard) ||
						f$IsTurnSet || (f$IsTurnStrongStraight && nsuitedcommon < 3) || f$IsTurnStrongFlush || f$IsStrongFull || f$IsTurnStrongPoker || isstraightflush ||
						(f$IsTurnStrongOverPair && !f$IsTurnDangerousBoard)
					:f$StackSize == 3 || f$StackSize == 4 ?
						(f$IsTurnStrongDrill && !f$IsFlopDangerousBoard) || (f$IsTurnSet && !f$IsFlopDangerousBoard) || 
						(f$IsTurnStrongStraight && nsuitedcommon < 3) || f$IsTurnStrongFlush || f$IsStrongFull || f$IsTurnStrongPoker || 
						isstraightflush
					:0
		:0
	:f$OpponentHandStrength == 0 ?
			f$IsTopPair || f$IsOverPair || f$IsTwoPair || f$IsDrill ||
			f$IsSet || f$IsStraight || f$IsFlush || f$IsFull || f$IsStrongPoker || isstraightflush
		
		:f$IsTurnTwoPair || f$IsTurnDrill ||
		f$IsTurnSet || f$IsTurnStraight || f$IsTurnFlush || f$IsTurnFull || f$IsTurnStrongPoker || isstraightflush ||
		f$IsTurnTopPair || f$IsTurnOverPair 
: //RAISE
	f$StackSize == 0 ?
			f$IsFlushDraw || f$IsTurnTwoPair || f$IsTurnDrill ||
			f$IsTurnSet || f$IsTurnStraight || f$IsTurnFlush || f$IsTurnFull || f$IsTurnStrongPoker || isstraightflush ||
			f$IsTurnStrongTopPair || f$IsTurnOverPair
	:f$StackSize == 1 ?
			f$IsTurnTwoPair || f$IsTurnDrill ||
			f$IsTurnSet || f$IsTurnStraight || f$IsTurnFlush || f$IsTurnFull || f$IsTurnStrongPoker || isstraightflush ||
			f$IsTurnStrongTopPair || f$IsTurnOverPair
	:f$StackSize == 2 ?
			pot < 40 * bblind ?
				(f$IsTurnStrongTwoPair && !f$IsFlopDangerousBoard) || (f$IsTurnStrongDrill && !f$IsFlopDangerousBoard) ||
				f$IsTurnSet || (f$IsTurnStrongStraight && nsuitedcommon < 3) || f$IsTurnStrongFlush || f$IsStrongFull || f$IsTurnStrongPoker || isstraightflush
		 :
				(f$IsTurnStrongTwoPair && !f$IsFlopDangerousBoard) || f$IsTurnDrill ||
				f$IsTurnSet || f$IsTurnStraight || f$IsTurnFlush || f$IsTurnFull || f$IsTurnStrongPoker || isstraightflush ||
				f$IsTurnStrongTopPair || f$IsTurnOverPair
	:f$StackSize == 3 || f$StackSize == 4 ?
			(f$IsTurnStrongDrill && !f$IsFlopDangerousBoard) || (f$IsTurnSet && nsuitedcommon < 3 && nstraightcommon < 3) || 
			(f$IsTurnStrongStraight && nsuitedcommon < 3) || f$IsTurnStrongFlush || f$IsStrongFull || f$IsTurnStrongPoker || 
			isstraightflush  
		:f$StackSize > 4 ?
		f$IsUltraStrongFull
:0

